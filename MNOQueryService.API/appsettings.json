{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Filename=mobilenetworkoperator.db"
  },
  "Redis": {
    "Configuration": "127.0.0.1:6379"
  },
  "AppOptimization": {
    "RedisSettings": {
      "OptimizeOperatorQuery": true,
      "CacheTimeInMinute": 1
    }
  }
}
